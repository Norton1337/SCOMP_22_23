#!/bin/bash

for config in "1 100" "1 500" "1 1000" "1 10000" "1 50000" "1 500000" "1 1000000" \
              "2 100" "2 500" "2 1000" "2 10000" "2 50000" "2 500000" "2 1000000" \
              "4 100" "4 500" "4 1000" "4 10000" "4 50000" "4 500000" "4 1000000" \
              "8 100" "8 500" "8 1000" "8 10000" "8 50000" "8 500000" "8 1000000" \
              "16 100" "16 500" "16 1000" "16 10000" "16 50000" "16 500000" "16 1000000"
do
    echo "Testing with config $config"
    for i in {1..10}
    do
        ./testpar $config | tee -a results.txt
    done
done
